{
    "manifest_version": 3,
    "name": "URI Decoder",
    "version": "1.0",
    "description": "URI Decoder.",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "icon.png"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
  